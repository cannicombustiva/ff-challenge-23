{
  "name": "ff-challenge-23",
  "version": "0.0.0",
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint",
    "*.css": "stylelint"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.svg": "<rootDir>/.jest/__mocks__/svgr-mock.ts",
      "^jest-helpers/(.*)": "<rootDir>/.jest/$1",
      "^components/(.*)": "<rootDir>/src/components/$1"
    },
    "setupFiles": [
      "<rootDir>/.jest/env.ts",
      "jest-canvas-mock"
    ],
    "transform": {
      "^.+\\.(tsx|ts|js|jsx)?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "cypress"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export -o dist",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "ts:check": "tsc --noEmit",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@types/react-slick": "0.23.10",
    "clsx": "1.2.1",
    "constate": "3.3.2",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-magic-slider-dots": "1.2.2",
    "react-rating-stars-component": "2.2.0",
    "react-slick": "0.29.0",
    "slick-carousel": "1.8.1",
    "swr": "2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@lasalefamine/eslint-config": "3.0.2",
    "@next/bundle-analyzer": "13.2.4",
    "@svgr/webpack": "7.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.31",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "cssnano": "6.0.0",
    "cssnano-preset-advanced": "6.0.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-cypress": "2.13.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-canvas-mock": "2.5.0",
    "lint-staged": "13.2.0",
    "next-images": "1.8.4",
    "postcss": "8.4.21",
    "postcss-easy-import": "4.0.0",
    "postcss-inset": "1.0.0",
    "postcss-mixins": "9.0.4",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "8.2.0",
    "postcss-responsive-type": "1.0.0",
    "react-test-renderer": "18.2.0",
    "stylelint": "15.3.0",
    "stylelint-config-equinusocio": "1.6.8",
    "stylelint-config-standard": "31.0.0",
    "ts-jest": "29.0.5",
    "typescript": "5.0.2"
  }
}
